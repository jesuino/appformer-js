{
  "name": "QS Chrome Extension",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "QS Chrome Extension",
  "chrome_url_overrides" : {
    "newtab": "home.html"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "codemirror.js",
        "contentscript.js"
      ],
      "matches": [
        "https://github.com/*"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
  ],
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "https://*/*",
    "http://*/*",
    "tabs",
    "declarativeContent",
    "background"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}