  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "publisher": "kiegroup",
  "main": "./dist/extension/extension.js",
  "contributes": {
